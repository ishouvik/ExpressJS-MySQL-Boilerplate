{
  "name": "blueprint",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "yarn db:setup && pm2-runtime start ecosystem.config.js --env production",
    "start:dev": "yarn db:setup && pm2-runtime start ecosystem.config.js --env development",
    "db:setup": "yarn db:create && yarn db:migrate",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "lint": "standard --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "standard --fix"
    }
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "helmet": "^3.15.0",
    "jsonapi-serializer": "^3.6.3",
    "morgan": "~1.9.0",
    "mysql2": "^1.6.4",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "husky": "^1.1.4",
    "standard": "^12.0.1"
  }
}
